BWIN2CRD	PROC
	;IN
	;DS:SI - SOURSE
	;ES:DI - DESTINATION
	;BP - LEN

	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	MOV	DX,3CEH
	MOV	AX,5
	OUT	DX,AX
	MOV	CX,4
	XOR	BX,BX
BWIN2CRD1:
	MOV	AL,4
	OUT	DX,AX
	MOV	AL,DS:[SI]
	MOV	ES:[DI+BX],AL
	ADD	BX,BP
	INC	AH
	LOOP	BWIN2CRD1
	POP	DX
	POP	CX
	POP	BX
	POP	AX
	RET
ENDP
